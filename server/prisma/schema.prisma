


generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

// Modelo que define a estrutura da coleção de usuários no MongoDB Atlas
model User {
  id    String @id @default(auto()) @map("_id") @db.ObjectId
  email String 
  name  String
  age   Int
}
